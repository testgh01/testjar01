<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<geronimo-plugin xmlns="http://geronimo.apache.org/xml/ns/plugins-1.3" xmlns:ns2="http://geronimo.apache.org/xml/ns/attributes-1.2">
    <name>Geronimo Plugins, Client :: Deployer</name>
    <category>Geronimo Core</category>
    <pluginGroup>false</pluginGroup>
    <description>Client plugin</description>
    <url>http://geronimo.apache.org/</url>
    <author>The Apache Geronimo development community</author>
    <license osi-approved="true">The Apache Software License, Version 2.0</license>
    <plugin-artifact>
        <module-id>
            <groupId>org.apache.geronimo.configs</groupId>
            <artifactId>client-deployer</artifactId>
            <version>2.2.1</version>
            <type>car</type>
        </module-id>
        <geronimo-version>2.2.1</geronimo-version>
        <jvm-version>1.5</jvm-version>
        <jvm-version>1.6</jvm-version>
        <dependency>
            <groupId>org.apache.geronimo.configs</groupId>
            <artifactId>j2ee-deployer</artifactId>
            <version>2.2.1</version>
            <type>car</type>
        </dependency>
        <dependency start="true">
            <groupId>org.apache.geronimo.configs</groupId>
            <artifactId>client</artifactId>
            <version>2.2.1</version>
            <type>car</type>
        </dependency>
        <dependency start="true">
            <groupId>org.apache.geronimo.configs</groupId>
            <artifactId>client-transaction</artifactId>
            <version>2.2.1</version>
            <type>car</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.modules</groupId>
            <artifactId>geronimo-client-builder</artifactId>
            <version>2.2.1</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.modules</groupId>
            <artifactId>geronimo-connector-builder</artifactId>
            <version>2.2.1</version>
            <type>jar</type>
        </dependency>
        <source-repository>~/.m2/repository/</source-repository>
        <source-repository>http://repo1.maven.org/maven2/</source-repository>
        <source-repository>http://repository.apache.org/snapshots/</source-repository>
        <copy-file dest-dir="schema">schema/schemaorg_apache_xmlbeans/src/</copy-file>
        <config-xml-content server="default"/>
        <config-xml-content server="offline"/>
        <artifact-alias server="client" key="org.apache.geronimo.configs/client-deployer//car">org.apache.geronimo.configs/client-deployer/2.2.1/car</artifact-alias>
        <artifact-alias server="client" key="org.apache.geronimo.configs/client-deployer/2.2/car">org.apache.geronimo.configs/client-deployer/2.2.1/car</artifact-alias>
    </plugin-artifact>
</geronimo-plugin>
