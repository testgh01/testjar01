<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<geronimo-plugin xmlns="http://geronimo.apache.org/xml/ns/plugins-1.3" xmlns:ns2="http://geronimo.apache.org/xml/ns/attributes-1.2">
    <name>Geronimo Plugins, Welcome :: Jetty</name>
    <category>Administration</category>
    <pluginGroup>false</pluginGroup>
    <description>The welcome application for Geronimo. 
       This binds to / so it handles all requests that nothing else is bound to. 
       The main purpose is to greet users who are exploring Geronimo for the first time. 
       It should typically be removed or replaced for production applications.</description>
    <url>http://geronimo.apache.org/</url>
    <author>The Apache Geronimo development community</author>
    <license osi-approved="true">The Apache Software License, Version 2.0</license>
    <plugin-artifact>
        <module-id>
            <groupId>org.apache.geronimo.configs</groupId>
            <artifactId>welcome-jetty</artifactId>
            <version>2.2.1</version>
            <type>car</type>
        </module-id>
        <geronimo-version>2.2.1</geronimo-version>
        <jvm-version>1.5</jvm-version>
        <jvm-version>1.6</jvm-version>
        <dependency>
            <groupId>org.apache.geronimo.configs</groupId>
            <artifactId>jasper</artifactId>
            <version>2.2.1</version>
            <type>car</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.framework</groupId>
            <artifactId>plugin</artifactId>
            <version>2.2.1</version>
            <type>car</type>
        </dependency>
        <source-repository>~/.m2/repository/</source-repository>
        <source-repository>http://repo1.maven.org/maven2/</source-repository>
        <source-repository>http://repository.apache.org/snapshots/</source-repository>
        <artifact-alias key="org.apache.geronimo.configs/welcome-jetty//car">org.apache.geronimo.configs/welcome-jetty/2.2.1/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/welcome-jetty/2.2/car">org.apache.geronimo.configs/welcome-jetty/2.2.1/car</artifact-alias>
    </plugin-artifact>
</geronimo-plugin>
