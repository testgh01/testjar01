        <dependency>
            <groupId>org.apache.mina</groupId>
            <artifactId>mina-core</artifactId>
            <version>1.1.6</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.mina</groupId>
            <artifactId>mina-filter-ssl</artifactId>
            <version>1.1.6</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>commons-vfs</groupId>
            <artifactId>commons-vfs</artifactId>
            <version>1.0</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.gshell.remote</groupId>
            <artifactId>gshell-remote-server</artifactId>
            <version>1.0-alpha-1</version>
            <type>jar</type>
        </dependency>
        <source-repository>~/.m2/repository/</source-repository>
        <source-repository>http://repo1.maven.org/maven2/</source-repository>
        <source-repository>http://repository.apache.org/snapshots/</source-repository>
        <artifact-alias key="org.apache.geronimo.framework/gshell-remote//car">org.apache.geronimo.framework/gshell-remote/2.2.1/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.framework/gshell-remote/2.2/car">org.apache.geronimo.framework/gshell-remote/2.2.1/car</artifact-alias>
