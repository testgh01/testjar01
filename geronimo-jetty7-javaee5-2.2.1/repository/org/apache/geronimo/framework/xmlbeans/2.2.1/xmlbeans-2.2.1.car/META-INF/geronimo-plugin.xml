<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<geronimo-plugin xmlns="http://geronimo.apache.org/xml/ns/plugins-1.3" xmlns:ns2="http://geronimo.apache.org/xml/ns/attributes-1.2">
    <name>Geronimo Framework, Configs :: XMLBeans</name>
    <category>Geronimo Core</category>
    <pluginGroup>false</pluginGroup>
    <description>Geronimo module that supplies the xmlbeans framework in a single classloader.</description>
    <url>http://geronimo.apache.org/</url>
    <author>The Apache Geronimo development community</author>
    <license osi-approved="true">The Apache Software License, Version 2.0</license>
    <plugin-artifact>
        <module-id>
            <groupId>org.apache.geronimo.framework</groupId>
            <artifactId>xmlbeans</artifactId>
            <version>2.2.1</version>
            <type>car</type>
        </module-id>
        <geronimo-version>2.2.1</geronimo-version>
        <jvm-version>1.5</jvm-version>
        <jvm-version>1.6</jvm-version>
        <dependency start="true">
            <groupId>org.apache.xmlbeans</groupId>
            <artifactId>xmlbeans</artifactId>
            <version>2.3.0</version>
            <type>jar</type>
        </dependency>
        <dependency start="true">
            <groupId>org.apache.geronimo.framework</groupId>
            <artifactId>j2ee-system</artifactId>
            <version>2.2.1</version>
            <type>car</type>
        </dependency>
        <source-repository>~/.m2/repository/</source-repository>
        <source-repository>http://repo1.maven.org/maven2/</source-repository>
        <source-repository>http://repository.apache.org/snapshots/</source-repository>
        <artifact-alias key="org.apache.geronimo.framework/xmlbeans//car">org.apache.geronimo.framework/xmlbeans/2.2.1/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.framework/xmlbeans/2.2/car">org.apache.geronimo.framework/xmlbeans/2.2.1/car</artifact-alias>
    </plugin-artifact>
</geronimo-plugin>
